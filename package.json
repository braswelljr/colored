{
  "name": "colored",
  "version": "1.0.0",
  "description": "Color palette ðŸŽ¨",
  "scripts": {
    "dev": "pnpm run write:asset && next dev",
    "build": "pnpm run write:asset && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "npx eslint ./**/*.{js,ts,jsx,tsx} --quiet --fix",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "format": "prettier --write ./**/*.{js,ts,jsx,tsx,css}",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{js,ts,jsx,tsx,css}\" --cache",
    "format:check": "prettier --check \"**/*.{js,ts,jsx,tsx,css}\" --cache",
    "write:asset": "node ./lib/generator.color.js && node ./lib/generator.pallete.js"
  },
  "keywords": [
    "color",
    "palette",
    "color-palette"
  ],
  "author": "Braswell Azu Kenneth Junior < [braswellkenneth7@gmail.com] >",
  "license": "MIT",
